@model Models.User



@{

    if (Model != null)
    {
        <div id="expanderContent">
            <table style='width: 100%'>
                <tr>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.FirstName)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.FirstName, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.FirstName)
                    </td>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.Password)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.Password, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.Password)
                    </td>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @*@Html.LabelFor(model => model.UserName)*@
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @*@Html.TextBoxFor(model => model.UserName, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.Firstname)*@
                    </td>
                </tr>
                <tr>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.LastName)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.LastName, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.LastName)
                    </td>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.EMail)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.EMail, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.EMail)
                    </td>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @*@Html.LabelFor(model => model.Inactive)*@
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @*@Html.TextBoxFor(model => model.Inactive, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.Inactive)*@
                    </td>
                </tr>
                <tr>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.UserName)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.UserName, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.UserName)
                    </td>
                    <td style='width: 10%; font-weight: bold' class='userTable'>
                        @Html.LabelFor(model => model.Inactive)
                    </td>
                    <td style='width: 28%' class='userTable'>
                        @Html.TextBoxFor(model => model.Inactive, new { @class = "singlelinetextinput" })
                        @Html.ValidationMessageFor(model => model.Inactive)
                    </td>
                </tr>
            </table>
        </div>
        @Html.HiddenFor(m => m.InsertDate)
    }
}
